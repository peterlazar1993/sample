{
  "version": 2,
  "projects": {
    "demo": {
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "projectType": "application",
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start demo"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": [
            "apps/demo/build"
          ],
          "options": {
            "entryFile": "apps/demo/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/demo/ios/main.bundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/demo/android/app/build/outputs/bundle",
            "apps/demo/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/demo/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/demo/android/main.bundle"
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "ensure-symlink": {
          "executor": "@nrwl/react-native:ensure-symlink",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/demo"
          ],
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "targets": {
        "build-ios": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "test-ios": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "ios.sim.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "ios.sim.release"
            }
          }
        },
        "build-android": {
          "executor": "@nrwl/detox:build",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "test-android": {
          "executor": "@nrwl/detox:test",
          "options": {
            "detoxConfiguration": "android.emu.debug"
          },
          "configurations": {
            "production": {
              "detoxConfiguration": "android.emu.release"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo-e2e/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react-native"
  },
  "defaultProject": "demo"
}